{
  "name": "InstaGhost",
  "version": "1.0",
  "description": "View instagram stories secretly",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}